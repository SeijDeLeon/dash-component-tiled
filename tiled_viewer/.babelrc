{
    "presets": [
      ["@babel/preset-env", { "modules": false }],
      "@babel/preset-react"
    ],
    "env": {
      "production": {
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread",
          "styled-jsx/babel"
        ]
      },
      "development": {
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread",
          "styled-jsx/babel"
        ]
      },
      "test": {
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread",
          "styled-jsx/babel-test"
        ]
      }
    }
  }
